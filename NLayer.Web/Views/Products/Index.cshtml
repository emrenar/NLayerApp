
@model List<ProductWithCategoryDto>

@{
    ViewData["Title"] = "Index";
}

 <table class="table table-striped">
     <thead>
         <tr>
         <td>Id</td>
         <td>Name</td>
         <td>Price</td>
         <td>Stock</td>
         <td>Category</td>
         <td>CreatedDate</td>
         </tr>
     </thead>
     <tbody>
         @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.ProductName</td>
                <td>@item.Price</td>
                <td>@item.Stock</td>
                <td>@item.Category.CategoryName</td>
                <td>@item.CreatedDate</td>
            </tr>
        }
     </tbody>

 </table>

